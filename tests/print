a := 1;
b := "asd";

while (a <= 5) do
begin
  print(a);
  a := a + 1
end;

print(b);

print("Input number:");
print(readint + 1);
print("Input string:");
print(readstr);

print((1+2*3)%5);
print(concatenate("asd", "asd"));
if (1 = 1) then print(1);
if ("asd" == "asd") then print("asd");
print(length("asd"));
print(position("abcdef", "cd"));
print(position("abcdef", "zxc"));

print(substring("abcdef", 3, 10));